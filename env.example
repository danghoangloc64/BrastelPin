# Server Configuration
SERVER_ID=1
TOTAL_SERVERS=5
PORT=3000

# Server Addresses - Replace with your actual VPS IPs
SERVER_1_ADDRESS=http://192.168.1.10:3000
SERVER_2_ADDRESS=http://192.168.1.11:3000
SERVER_3_ADDRESS=http://192.168.1.12:3000
SERVER_4_ADDRESS=http://192.168.1.13:3000
SERVER_5_ADDRESS=http://192.168.1.14:3000

# NTFY Notification Configuration
NTFY_TOPIC=dhloc
NTFY_SERVER=https://ntfy.sh
NTFY_TITLE=Brastel PIN Checker

# PIN Checker Configuration
CONCURRENT_WORKERS=1
MAX_RETRIES=10
RETRY_DELAY=3000
PROXY_ROTATION_INTERVAL=250000
REQUEST_TIMEOUT=60000
MAX_UNDEFINED_RESULTS=25
RANDOM_PROCESSING=true
DELAY_BETWEEN_PINS=100

# Folders
LOG_FOLDER=Log
DATA_FOLDER=Data

# API Configuration (optional - leave empty to use defaults)
# API_URL=https://www.brastel.com/web/WIMS/Manager.aspx
# USER_AGENT=Mozilla/5.0 (iPhone; CPU iPhone OS 16_6 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/16.6 Mobile/15E148 Safari/604.1
# REFERER=https://www.brastel.com/myaccount/m/password/eng
# ORIGIN=https://www.brastel.com

# Proxy List (comma-separated, optional)
# PROXY_LIST=http://proxy1:port,http://proxy2:port,http://proxy3:port

# Cookie List (pipe-separated, optional)
# COOKIE_LIST=cookie1|cookie2|cookie3

# Example for different ports
# SERVER_1_ADDRESS=http://vps1.example.com:8080
# SERVER_2_ADDRESS=http://vps2.example.com:8080

# Example for HTTPS
# SERVER_1_ADDRESS=https://vps1.example.com:443
# SERVER_2_ADDRESS=https://vps2.example.com:443